MCP_GATEWAY_PORT=3082

# MongoDB Configuration (shared with LibreChat)
MCP_GATEWAY_MONGO_URL=mongodb://localhost:27017
MCP_GATEWAY_MONGO_DB_NAME=mcp_webui

# Security Configuration
MCP_GATEWAY_JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Proxy Configuration
MCP_GATEWAY_PROXY_PREFIX=/mcp

# CORS Configuration
MCP_GATEWAY_CORS_ORIGINS=*,http://dev.sizzek.dungeonmind.net,https://dev.sizzek.dungeonmind.net

# Logging
MCP_GATEWAY_ENABLE_LOGGING=true

# Development Settings
NODE_ENV=development
MCP_WEB_UI_USE_GATEWAY=true
